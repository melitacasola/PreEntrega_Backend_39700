<div>
    <h1>Products Manager</h1>
    <ul>
        {{#each prods}}
        <li>
            <p><b>{{title}}</b></p>
            <p>Precio: ${{price}}</p>
            <p>Descripción: {{description}}</p>
            <p>Stock: {{stock}}</p>
            <p>Categoría: {{category}}</p>
        </li>

        {{/each}}
    </ul>
</div>